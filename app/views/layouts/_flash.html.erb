<%- if notice %>

  <div class="notice-box text-center">
    <div class="box-contents">
     <p class="notice"><%= notice %></p>
    </div>
  </div>

<%- end %>

<%- if alert %>

  <div class="alert-box text-center">
    <div class="box-contents">
      <p class="alert"><%= alert %></p>
    </div>
  </div>

<%- end %>